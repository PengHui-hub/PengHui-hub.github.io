	date-added = {2025-04-02 14:21:03 +0800},
	date-modified = {2025-04-02 14:21:03 +0800},
	doi = {10.1093/mnras/stac2713},
	eprint = {2203.14609},
	journal = {\mnras},
	keywords = {methods: data analysis, galaxies: photometry, large-scale structure of Universe, Astrophysics - Cosmology and Nongalactic Astrophysics},
	month = nov,
	number = {4},
	pages = {6210-6224},
	primaryclass = {astro-ph.CO},
	title = {{Self-calibration of photometric redshift scatter from DECaLS DR8 power spectrum and validation with simulated catalogues}},
