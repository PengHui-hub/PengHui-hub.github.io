	doi = {10.1093/mnras/stad2808},
	eprint = {2305.10487},
	journal = {\mnras},
	keywords = {line: identification, methods: data analysis, large-scale structure of Universe, Astrophysics - Cosmology and Nongalactic Astrophysics},
	month = nov,
	number = {1},
	pages = {820-829},
	primaryclass = {astro-ph.CO},
	title = {{Precise self-calibration of interloper bias in spectroscopic surveys}},
